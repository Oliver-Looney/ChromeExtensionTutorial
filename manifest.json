{
    "manifest_version": 3,
    "name": "Chrome Demo",
    "description": "my description",
    "version": "1.0",
    "background":{
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "./images/get_started128.png",
            "32": "./images/get_started128.png",
            "48": "./images/get_started128.png",
            "128": "./images/get_started128.png"
        }
    },
    "icons": {
        "16": "./images/get_started128.png",
        "32": "./images/get_started128.png",
        "48": "./images/get_started128.png",
        "128": "./images/get_started128.png"
    },
    "permissions": [
     "storage"
     ]
  }